{
    "manifest_version": 2,
    "name": "WordReplace",
    "version": "0.1",
  
    "description": "Search and replaces words on a page automatically",
  
    "icons": {
      "48": "icons/icon-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_scripts/wordreplace.js"]
      }
    ]
  
  }
  