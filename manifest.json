{
    "manifest_version": 2,
    "name": "WordReplace",
    "version": "0.1",
  
    "description": "Search and replaces words on a page automatically",
  
    "icons": {
      "48": "icons/icon-48.png",
      "92": "icons/nonexistent-96.png"
    },

    "permissions": [
        "activeTab"
    ],

    "browser_action": {
        "default_icon": "icons/icon-32x32.png",
        "default_title": "WordReplace",
        "default_popup": "popup/popup.html"
    },

    "web_accessible_resources" : []
  }